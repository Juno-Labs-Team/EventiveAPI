{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Supabase": {
    "Url": "#{SUPABASE_URL}#",
    "ServiceRoleKey": "#{SUPABASE_SERVICE_ROLE_KEY}#",
    "AnonKey": "#{SUPABASE_ANON_KEY}#"
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:5173", "https://eventive-tsa.com"]
  },
  "Storage": {
    "AvatarBucket": "avatars",
    "MaxFileSize": 5242880
  },
  "RateLimit": {
    "EnableRateLimiting": true,
    "PermitLimit": 100,
    "Window": "00:15:00",
    "QueueLimit": 0
  },
  "Authentication": {
    "UseCustomAuth": false
  }
}
